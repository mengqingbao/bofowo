<div class="row">
<div class="box col-md-12">
	<div class="box-inner">
		<div class="box-header well" data-original-title="">
			<h2>Comments Management</h2>
			<div class="box-icon">
				<a href="#" class="btn btn-minimize btn-round btn-default"><i
						class="glyphicon glyphicon-chevron-up"></i></a>
				<a href="#" class="btn btn-close btn-round btn-default"><i
						class="glyphicon glyphicon-remove"></i></a>
			</div>
		</div>
		<div class="box-content">

<table class="table table-striped table-bordered responsive">
	<tr>
		<td><label for="status">#springMessage("id")</label></td>
		<td>
			$!{comments.id}
		</td>
	</tr>
	<tr>
		<td><label for="status">#springMessage("topicId")</label></td>
		<td>
			$!{comments.topicId}
		</td>
	</tr>
	<tr>
		<td><label for="status">#springMessage("comments")</label></td>
		<td>
			$!{comments.comments}
		</td>
	</tr>
	<tr>
		<td><label for="status">#springMessage("creator")</label></td>
		<td>
			$!{comments.creator}
		</td>
	</tr>
	<tr>
		<td><label for="status">#springMessage("createDate")</label></td>
		<td>
			$!{comments.createDate}
		</td>
	</tr>
	<tr>
		<td><label for="status">#springMessage("changeMan")</label></td>
		<td>
			$!{comments.changeMan}
		</td>
	</tr>
	<tr>
		<td><label for="status">#springMessage("changeDate")</label></td>
		<td>
			$!{comments.changeDate}
		</td>
	</tr>
	<tr>
		<td><label for="status">#springMessage("status")</label></td>
		<td>
			$!{comments.status}
		</td>
	</tr>
	<tr>
		<td><label for="status">#springMessage("title")</label></td>
		<td>
			$!{comments.title}
		</td>
	</tr>
</table>

</div><!--box-content end-->
	</div>
</div>
<!--/span-->

</div><!--/row-->