<div class="row">
<div class="box col-md-12">
	<div class="box-inner">
		<div class="box-header well" data-original-title="">
			<h2>Account Management</h2>
			<div class="box-icon">
				<a href="#" class="btn btn-minimize btn-round btn-default"><i
						class="glyphicon glyphicon-chevron-up"></i></a>
				<a href="#" class="btn btn-close btn-round btn-default"><i
						class="glyphicon glyphicon-remove"></i></a>
			</div>
		</div>
		<div class="box-content">

<table class="table table-striped table-bordered responsive">
	<tr>
		<td><label for="status">#springMessage("id")</label></td>
		<td>
			$!{account.id}
		</td>
	</tr>
	<tr>
		<td><label for="status">#springMessage("username")</label></td>
		<td>
			$!{account.username}
		</td>
	</tr>
	<tr>
		<td><label for="status">#springMessage("password")</label></td>
		<td>
			$!{account.password}
		</td>
	</tr>
	<tr>
		<td><label for="status">#springMessage("createDate")</label></td>
		<td>
			$!{account.createDate}
		</td>
	</tr>
	<tr>
		<td><label for="status">#springMessage("status")</label></td>
		<td>
			$!{account.status}
		</td>
	</tr>
	<tr>
		<td><label for="status">#springMessage("type")</label></td>
		<td>
			$!{account.type}
		</td>
	</tr>
	<tr>
		<td><label for="status">#springMessage("email")</label></td>
		<td>
			$!{account.email}
		</td>
	</tr>
	<tr>
		<td><label for="status">#springMessage("emailValid")</label></td>
		<td>
			$!{account.emailValid}
		</td>
	</tr>
	<tr>
		<td><label for="status">#springMessage("mobile")</label></td>
		<td>
			$!{account.mobile}
		</td>
	</tr>
	<tr>
		<td><label for="status">#springMessage("mobileValid")</label></td>
		<td>
			$!{account.mobileValid}
		</td>
	</tr>
	<tr>
		<td><label for="status">#springMessage("isValid")</label></td>
		<td>
			$!{account.isValid}
		</td>
	</tr>
</table>

</div><!--box-content end-->
	</div>
</div>
<!--/span-->

</div><!--/row-->