<div class="row">
<div class="box col-md-12">
	<div class="box-inner">
		<div class="box-header well" data-original-title="">
			<h2>Video Management</h2>
			<div class="box-icon">
				<a href="#" class="btn btn-minimize btn-round btn-default"><i
						class="glyphicon glyphicon-chevron-up"></i></a>
				<a href="#" class="btn btn-close btn-round btn-default"><i
						class="glyphicon glyphicon-remove"></i></a>
			</div>
		</div>
		<div class="box-content">

<table class="table table-striped table-bordered responsive">
	<tr>
		<td><label for="status">#springMessage("id")</label></td>
		<td>
			$!{video.id}
		</td>
	</tr>
	<tr>
		<td><label for="status">#springMessage("title")</label></td>
		<td>
			$!{video.title}
		</td>
	</tr>
	<tr>
		<td><label for="status">#springMessage("videoHtml")</label></td>
		<td>
			$!{video.videoHtml}
		</td>
	</tr>
	<tr>
		<td><label for="status">#springMessage("tpicId")</label></td>
		<td>
			$!{video.tpicId}
		</td>
	</tr>
	<tr>
		<td><label for="status">#springMessage("creator")</label></td>
		<td>
			$!{video.creator}
		</td>
	</tr>
	<tr>
		<td><label for="status">#springMessage("order")</label></td>
		<td>
			$!{video.order}
		</td>
	</tr>
	<tr>
		<td><label for="status">#springMessage("status")</label></td>
		<td>
			$!{video.status}
		</td>
	</tr>
	<tr>
		<td><label for="status">#springMessage("type")</label></td>
		<td>
			$!{video.type}
		</td>
	</tr>
	<tr>
		<td><label for="status">#springMessage("createDate")</label></td>
		<td>
			$!{video.createDate}
		</td>
	</tr>
</table>

</div><!--box-content end-->
	</div>
</div>
<!--/span-->

</div><!--/row-->