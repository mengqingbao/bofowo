<div class="row">
<div class="box col-md-12">
	<div class="box-inner">
		<div class="box-header well" data-original-title="">
			<h2>Page Management</h2>
			<div class="box-icon">
				<a href="#" class="btn btn-minimize btn-round btn-default"><i
						class="glyphicon glyphicon-chevron-up"></i></a>
				<a href="#" class="btn btn-close btn-round btn-default"><i
						class="glyphicon glyphicon-remove"></i></a>
			</div>
		</div>
		<div class="box-content">

<table class="table table-striped table-bordered responsive">
	<tr>
		<td><label for="status">#springMessage("id")</label></td>
		<td>
			$!{page.id}
		</td>
	</tr>
	<tr>
		<td><label for="status">#springMessage("name")</label></td>
		<td>
			$!{page.name}
		</td>
	</tr>
	<tr>
		<td><label for="status">#springMessage("url")</label></td>
		<td>
			$!{page.url}
		</td>
	</tr>
	<tr>
		<td><label for="status">#springMessage("type")</label></td>
		<td>
			$!{page.type}
		</td>
	</tr>
	<tr>
		<td><label for="status">#springMessage("status")</label></td>
		<td>
			$!{page.status}
		</td>
	</tr>
	<tr>
		<td><label for="status">#springMessage("layout")</label></td>
		<td>
			$!{page.layout}
		</td>
	</tr>
	<tr>
		<td><label for="status">#springMessage("themeId")</label></td>
		<td>
			$!{page.themeId}
		</td>
	</tr>
	<tr>
		<td><label for="status">#springMessage("content")</label></td>
		<td>
			$!{page.content}
		</td>
	</tr>
</table>

</div><!--box-content end-->
	</div>
</div>
<!--/span-->

</div><!--/row-->